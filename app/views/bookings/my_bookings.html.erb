<h1>My bookings:</h1>

<% if @bookings %>
  <ul>
    <% @bookings.each do |booking| %>
      <li><%= booking.bike.brand %> || <%= booking.booking_start %> ||
      <%= booking.booking_end %> || <%= booking.total_price %>
      <%= link_to "Delete?", article_path(@article.id), method: :delete, data: { confirm: "Are you sure?" }  %>
      </li>
    <% end %>
  </ul>
<% end %>
